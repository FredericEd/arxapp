{
  "expo": {
    "name": "Arx",
    "slug": "arxsmart",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tymsmart.arx"
    },
    "android": {
      "package": "com.tymsmart.arx",
      "googleServicesFile": "./google-services.json"
    },
    "notification": {
      "icon": "./assets/icon.png"
    }
  }
}